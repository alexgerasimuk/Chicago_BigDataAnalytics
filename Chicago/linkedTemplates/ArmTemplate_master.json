{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Chicago"
		},
		"AffordableHousingBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AffordableHousingBlob'"
		},
		"AffordableHpusingBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AffordableHpusingBlob'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CrimesBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CrimesBlob'"
		},
		"PermitsBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PermitsBlob'"
		},
		"TrafficBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TrafficBlob'"
		},
		"RestServiceAffordableHousing_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/s6ha-ppgi.json?$select=community_area,community_area_number,property_type,units"
		},
		"RestServiceCrimes_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/crimes.json?$select=id,date, year, community_area, primary_type, arrest, domestic"
		},
		"RestServicePermit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/building-permits.json?$select=id,permit_type,issue_date,community_area"
		},
		"RestServiceTraffic_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/kf7e-cur8.json?$select=time, region_id, speed, region, bus_count"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Chicago_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AffordableHousingBlob_connectionString": {
						"value": "[parameters('AffordableHousingBlob_connectionString')]"
					},
					"AffordableHpusingBlob_connectionString": {
						"value": "[parameters('AffordableHpusingBlob_connectionString')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"CrimesBlob_connectionString": {
						"value": "[parameters('CrimesBlob_connectionString')]"
					},
					"PermitsBlob_connectionString": {
						"value": "[parameters('PermitsBlob_connectionString')]"
					},
					"TrafficBlob_connectionString": {
						"value": "[parameters('TrafficBlob_connectionString')]"
					},
					"RestServiceAffordableHousing_properties_typeProperties_url": {
						"value": "[parameters('RestServiceAffordableHousing_properties_typeProperties_url')]"
					},
					"RestServiceCrimes_properties_typeProperties_url": {
						"value": "[parameters('RestServiceCrimes_properties_typeProperties_url')]"
					},
					"RestServicePermit_properties_typeProperties_url": {
						"value": "[parameters('RestServicePermit_properties_typeProperties_url')]"
					},
					"RestServiceTraffic_properties_typeProperties_url": {
						"value": "[parameters('RestServiceTraffic_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}