{
	"name": "chicago_dataflow",
	"properties": {
		"activities": [
			{
				"name": "Copy traffic",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"blockSizeInMB": 100
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "arrayOfObjects"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['time']"
								},
								"sink": {
									"path": "time"
								}
							},
							{
								"source": {
									"path": "$['region_id']"
								},
								"sink": {
									"path": "region_id"
								}
							},
							{
								"source": {
									"path": "$['speed']"
								},
								"sink": {
									"path": "speed"
								}
							},
							{
								"source": {
									"path": "$['region']"
								},
								"sink": {
									"path": "region"
								}
							},
							{
								"source": {
									"path": "$['bus_count']"
								},
								"sink": {
									"path": "bus_count"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "RestResourceTraffic",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "traffic",
						"type": "DatasetReference",
						"parameters": {
							"TargetFileName": {
								"value": "traffic/@concat('traffic_', formatDateTime(dataset().WindowStartTime, 'MM-dd-yyyy_hh-mm-ss'))",
								"type": "Expression"
							}
						}
					}
				]
			},
